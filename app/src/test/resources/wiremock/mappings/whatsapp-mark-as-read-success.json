{
  "name": "Mark WhatsApp Message as Read - Success",
  "request": {
    "method": "POST",
    "urlPathPattern": "/v22.0/[^/]+/messages",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.status",
        "equalTo": "read"
      },
      {
        "matchesJsonPath": "$.messaging_product",
        "equalTo": "whatsapp"
      },
      {
        "matchesJsonPath": "$.message_id"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "success": true
    }
  }
} 